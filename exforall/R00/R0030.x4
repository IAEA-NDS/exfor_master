          3(35-BR-79(P,X)35-BR-76,CUM,TTY,,DT,DERIV)
          4(35-BR-79(P,X)35-BR-77,CUM,TTY,,DT,DERIV)
REL-REF    (R,,D.De.Jong+,J,ARI,30,188,1979)
